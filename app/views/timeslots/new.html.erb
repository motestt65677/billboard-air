<h1>New Timeslot</h1>
<%=form_for [@board, @timeslot] do |f|%>
  <div>
    <%=f.label :start_date%><br>
    <%=f.date_field :start_date%>
  </div>
  <div>
    <%=f.label :end_date%><br>
    <%=f.date_field :end_date%>
  </div>
  <div>
    <%=f.label "starting price"%><br>
    <%=f.number_field :current_price%>
  </div>
  <%=f.submit "Create"%>
<%end%>
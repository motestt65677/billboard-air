<%=form_for @board do |f|%>
  <div>
    <%=f.label :title%>
    <%=f.text_field :title%>
  </div>
  <div>
    <%=f.label :description%>
    <%=f.text_field :description%>
  </div>
  <div>
    <%=f.label :location%>
    <%=f.text_field :location%>
  </div>
  <div>
    <%=f.file_field :images, multiple:true%>
  </div>
  <%=f.submit "Update"%>  
<%end%>
